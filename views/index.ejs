<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="icon" href="../../../../favicon.ico">
    <title>DESII APP</title>
    <!-- Bootstrap core CSS -->
    <link href="/browser_components/bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    <!-- Custom styles for this template -->
    <link href="/stylesheets/signin.css" rel="stylesheet">
    <script src="/frontend/js/jquery-3.2.1.min.js"></script>
  </head>

  <body class="text-center">
    <form class="form-signin">
      <input type="hidden" name="_csrf" id="csrf" value="<%=locals.csrfToken%>">
      <h1 class="h3 mb-3 font-weight-normal">Please sign in</h1>
      <label for="inputEmail" class="sr-only">User ID</label>
      <input type="text" id="UserID" class="form-control" placeholder="UserId" required autofocus autocomplete="off">
      <label for="inputPassword" class="sr-only">Password</label>
      <input type="password" id="Password" class="form-control" placeholder="Password" required autocomplete="off">
      <div class="checkbox mb-3">
         <p id="msg" class=""></p>
     </div>
      <button class="btn btn-lg btn-primary btn-block" id="submit">Sign in</button>
    </form>
  </body>
</html>
<script src="/javascripts/login.js"></script>
<script src="/frontend/Scripts/sha256.js"></script>
